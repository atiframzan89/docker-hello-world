{
  "family": "wordpress",
  "executionRoleArn": "arn:aws:iam::057632058329:role/wordpress-dev-ecs-exec",
  "containerDefinitions": [
      {
          "name": "wordpress-dev-ecs",
          "image": "connecttesth.jfrog.io/docker/wordpress:latest",
          "cpu": 512,
          "portMappings": [
            {
              "hostPort": 8080,
              "protocol": "tcp",
              "containerPort": 8080
            }
          ],
          "memory": 1024,
          "essential": true,
          "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
              "awslogs-group": "wordpress-dev-ecs",
              "awslogs-region": "us-east-1",
              "awslogs-stream-prefix": "dev"
            }
          }
      }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "2048"
}
